{
  "name": "be-nc-news",
  "version": "1.0.0",
  "description": "bc-nc-news",
  "main": "index.js",
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "setup-dbs": "psql -f ./db/setup.sql",
    "make-migrate": "knex migrate:make",
    "migrate-latest": "knex migrate:latest",
    "migrate-rollback": "knex migrate:rollback",
    "seed": "knex seed:run",
    "seed-test": "NODE_ENV=test knex seed:run",
    "view-dev": "psql -f ./db/viewdb/dev.sql > ./db/viewdb/dev.db.txt",
    "view-test": "NODE_ENV=test psql -f ./db/viewdb/test.sql > ./db/viewdb/test.db.txt",
    "test-utils": "mocha spec/utils.spec.js",
    "test": "NODE_ENV=test mocha spec/app.spec.js",
    "seed:prod": "NODE_ENV=production DB_URL=$(heroku config:get DATABASE_URL) knex seed:run",
    "migrate-latest:prod": "NODE_ENV=production DB_URL=$(heroku config:get DATABASE_URL) knex migrate:latest",
    "migrate-rollback:prod": "NODE_ENV=production DB_URL=$(heroku config:get DATABASE_URL) knex migrate:rollback",
    "start": "node listen.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/northcoders/be-nc-news.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/northcoders/be-nc-news/issues"
  },
  "homepage": "https://github.com/northcoders/be-nc-news#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "knex": "^2.4.0",
    "pg": "^7.18.2"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^9.2.0",
    "sams-chai-sorted": "^1.0.2",
    "supertest": "^4.0.2"
  }
}
